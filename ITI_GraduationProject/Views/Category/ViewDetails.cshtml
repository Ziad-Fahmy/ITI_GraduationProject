@model Category

<div class=" card border-dark mb-3 text-white bg-secondary mb-3" style="max-width: 27rem;">
    <div class="card-header">Category Details</div>

    <div class="card-body">
        <h4 class="card-title">Category Name: @Model.Name</h4>
        <h5 class="card-subtitle mb-2 ">Category Id: @Model.Id</h5>
        <h5 class="card-subtitle mb-2 "> Description: @Model.Description</h5>

    </div>


    <div class="d-flex justify-content-center">
        <a asp-controller="Category" asp-action="Index" class="btn btn-dark btn-lg mb-2">Back</a>
    </div>
</div>